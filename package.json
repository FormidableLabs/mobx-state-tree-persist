{
  "name": "mobx-state-tree-persist",
  "version": "0.0.2",
  "license": "MIT",
  "author": {
    "name": "Formidable",
    "url": "https://formidable.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/mobx-state-tree-persist.git"
  },
  "homepage": "https://github.com/FormidableLabs/mobx-state-tree-persist",
  "keywords": [
    "mobx-state-tree",
    "persist",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ],
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --quiet",
    "check:ci": "yarn typecheck && yarn lint",
    "preversion": "yarn check:ci",
    "build": "tsup",
    "prepare": "yarn build",
    "changeset": "changeset",
    "version": "yarn changeset version && yarn install --frozen-lockfile"
  },
  "peerDependencies": {
    "mobx-state-tree": "^5.1.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mobx": "^6.7.0",
    "mobx-state-tree": "^5.1.7",
    "prettier": "^2.7.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">= 14"
  }
}
